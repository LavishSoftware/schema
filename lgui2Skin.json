{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "type":"object",
    "title":"LavishGUI 2 Skin",
    "description": "A LavishGUI 2 Skin containing templates",
    "properties":{
        "name":{
            "type":"string",
            "description":"The Name of the Skin"
        },
        "templates":{
            "type":"object",
            "description": "A set Templates to add to the Skin. Each Template can be referred to by the name provided here to define it."
        }
    },
    "required":["name"]
}
